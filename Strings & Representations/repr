class Point3D:
    def __init__(self,x,y,z):
        self.x = x
        self.y = y
        self.z =z

    def __str__(self):
        return '({},{},{})'.format(self.x,self.y,self.z)

    def __rep__(self):
        return 'Point3D(x={},y={},z={}'

p1 = Point3D(4,5,6)
p2 = Point3D(7,8,9)

repr(p1)
repr(p2)